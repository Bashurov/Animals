    url('../fonts/Bangers-Regular.woff2') format('woff2'),
    url('../fonts/Bangers-Regular.woff') format('woff'),
    url('../fonts/Bangers-Regular.ttf') format('truetype'),
    url('../fonts/Bangers-Regular.svg#Bangers-Regular') format('svg');
  font-weight: normal;
  font-style: normal;
}


